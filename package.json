{
	"publisher": "Kmzx",
	"author": {
		"name": "ZeKmzx"
	},
	"name": "zegit-submodules-manager",
	"displayName": "ZeGit Submodules Manager",
	"description": "Supercharged Git: manage Git submodules",
	"version": "0.1.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/mlaidouni/zegit-submodules-manager.git"
	},
	"engines": {
		"vscode": "^1.94.0"
	},
	"categories": [
		"SCM Providers"
	],
	"bugs": {
		"url": "https://github.com/mlaidouni/zegit-submodules-manager/issues"
	},
	"keywords": [
		"submodule",
		"submodules",
		"git submodule",
		"git submodules",
		"git",
		"zemizer"
	],
	"pricing": "Free",
	"icon": "resources/icon.png",
	"main": "src/extension.js",
	"activationEvents": [],
	"contributes": {
		"commands": [
			{
				"command": "zegit-submodules-manager.helloWorld",
				"title": "Hello World",
				"category": "ZeGit"
			},
			{
				"command": "zegit-submodules-manager.initAllSubmodules",
				"title": "Initialize All Submodules",
				"category": "ZeGit"
			},
			{
				"command": "zegit-submodules-manager.initSpecificSubmodule",
				"title": "Initialize Specific Submodule",
				"category": "ZeGit"
			},
			{
				"command": "zegit-submodules-manager.deinitAllSubmodules",
				"title": "Deinitialize All Submodules",
				"category": "ZeGit"
			},
			{
				"command": "zegit-submodules-manager.deinitSpecificSubmodule",
				"title": "Deinitialize Specific Submodule",
				"category": "ZeGit"
			},
			{
				"command": "zegit-submodules-manager.refreshSubmodules",
				"title": "Refresh Submodules View",
				"category": "ZeGit"
			}
		],
		"views": {
			"zegit": [
				{
					"id": "submodulesView",
					"name": "Submodules"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "zegit",
					"title": "ZeGit",
					"icon": "resources/git.svg"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.9",
		"@types/node": "22.x",
		"@types/vscode": "^1.94.0",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.4.1",
		"eslint": "^9.13.0",
		"typescript": "^5.6.3"
	}
}